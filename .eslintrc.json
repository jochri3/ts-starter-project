 {
   "env": {
     "browser": true,
     "es2021": true
   },
   "extends": [
     "eslint:recommended",
     "plugin:@typescript-eslint/recommended",
     "prettier"
   ],
   "parser": "@typescript-eslint/parser",
   "parserOptions": {
     "ecmaVersion": "latest",
     "sourceType": "module"
   },
   "plugins": ["@typescript-eslint"],
   "rules": {
     "no-else-return": "error",
     "max-statements": ["error", 20],
     "max-params": ["error", 3],
     "no-unused-vars": "error"
   }
 }
